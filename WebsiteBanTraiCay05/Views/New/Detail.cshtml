@model WebsiteBanTraiCay05.Models.EF.New
@{
    ViewBag.Title = Model.SeoTitle;
    ViewBag.SeoDescription = Model.SeoDescription;
    ViewBag.SeoKeyWord = Model.SeoKeywords;
}
<div class="container" style="margin-top: 180px">
    @if (Model != null)
    {
        <div class="row">
            <div class="col-md-12">
                <h1 class="border-bottom mb-5 pt-3">@Model.Title</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                @Model.Description
            </div>
            <div class="col-md-12">
                @Html.Raw(Model.Detail)
            </div>
        </div>
    }
</div>
